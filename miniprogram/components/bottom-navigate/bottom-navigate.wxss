/* 底部导航start */
.nav-bottom-background {
  width: 100%;
  height: 168rpx;
  background: white;
  box-shadow: 0rpx -4rpx 60rpx 0rpx rgba(0, 0, 0, 0.09), 0rpx 4rpx 8rpx 0rpx rgba(0, 0, 0, 0.09);
  position: fixed;
  bottom: 0;
  z-index: 997;
}
.nav-bottom-circle {
  width: 134rpx;
  height: 134rpx;
  position: fixed;
  bottom: 67rpx;
  left: 308rpx;
  background: white;
  box-shadow: 0rpx -4rpx 60rpx 0rpx rgba(0, 0, 0, 0.09), 0rpx 4rpx 8rpx 0rpx rgba(0, 0, 0, 0.09);
  border-radius: 50%;
  z-index: 998;
  float: left;
}
.nav-bottom {
  width: 100%;
  height: 168rpx;
  background: white;
  position: fixed;
  bottom: 0;
  z-index: 999;
  border: none;
}
.nav-bottom-view {
  width: 20%;
  height: 168rpx;
  background: white;
  float: left;
  border: none;
  text-align: center;
}
.nav-bottom-image {
  width: 44rpx;
  height: 44rpx; 
  margin-top: 22rpx;
}
.nav-bottom-text {
  margin-top: 8rpx;
  width: 100%;
  height: 20rpx;
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(0, 0, 0, 0.24);
  line-height: 20rpx;
  float: left;
  text-align: center;
  vertical-align: middle;
}
.nav-bottom-text-on {
  color: rgba(0, 0, 0, 0.87);
}
.nav-bottom-image-mid{
  width: 104rpx;
  height: 104rpx; 
  border-radius: 50%;
  margin-top: -22rpx;
}
/* 底部导航end */