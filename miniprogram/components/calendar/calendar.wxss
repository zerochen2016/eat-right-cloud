.cal{
  background-color: white;
  width: 750rpx;
  
}
.cal-head{
  height: 100rpx;
  line-height: 100rpx;
  width: 100%;
  float: left;
  background-color: white;
}
.cal-head-picker{
  width: 436rpx;
  text-align: center;
  float: left;
  font-size: 36rpx;
  font-weight: 500;
  line-height: 100rpx;
}
.cal-head image{
  width: 50rpx;
  height: 50rpx;
  float: left;
  margin-top: 25rpx;
}
.cal-body{
  width: 750rpx;
  float: left;
  background-color: white;
}
.cal-body-week-head{
  width: 100rpx;
  height: 100rpx;
  float: left;
  line-height: 100rpx;
  text-align: center;
  float: left;
  font-weight: 500;
  font-size: 30rpx;
}
.cal-body-week-days{
  width: 750rpx;
  background-color: white;
  position: relative;
  float: left;
}
.cal-body-week{
  width: 100rpx;
  height: 100rpx;
  float: left;
  line-height: 100rpx;
  text-align: center;
  float: left;
  font-weight: 400;
  font-size: 36rpx;
  border-radius: 8rpx;
  margin-top: 5rpx;
  margin-bottom: 5rpx;
}
.cal-body-week text{
  width: 100%;
  font-size: 22rpx;
  line-height: 22rpx;
  margin-top: -30rpx;
  float: left;
}
.cal-body-week-grey{
  color: rgba(204, 204, 204, 1);
}
.cal-body-week-report{
  color: rgba(0, 149, 145, 1);
  background-color: rgba(0, 149, 145, 0.1);
}
.cal-body-week-select{
  background-color: rgba(0, 149, 145, 1);
  color: white;
}
.cal-line{
  background-color: rgba(238, 238, 238, 1);
  height: 2rpx;
  width: 750rpx;
  float: left;
}