page{
  width: 100%;
  height: 1200rpx;
}
.head{
  margin-top: 32rpx;
  width: 100%;
  height: 90rpx;
  display: inline-block;
}
.head-text{
  margin-left: 40rpx;
  float: left;
  line-height: 90rpx;
}
.head-avatar{
  float: right;
  margin-right: 40rpx;
}
.new{
  width: 100%;
  height: 90rpx;
  margin-top: 68rpx;
}
.new-text-1{
  margin-left: 40rpx;
  margin-top: 20rpx;
  color: #F5A200;
  float: left;
}
.new-text-2{
  float: right;
  margin-right: 40rpx;
  width: 354rpx;
  height: 88rpx;
  background-color: rgba(205, 243, 228, 1);
  line-height: 68rpx;
  border-radius: 30rpx 30rpx 0rpx 0rpx;
  color: #00B386;
  text-align: center;
}
.new-image{
  width: 36rpx;
  height: 36rpx;
  float: left;
  margin-top: -6rpx
}
.check-link{
  width: 670rpx;
  height: 536rpx;
  background: white;
  box-shadow: 0rpx 8rpx 20rpx 0rpx rgba(0, 0, 0, 0.02), 0rpx -8rpx 20rpx 0rpx rgba(0, 0, 0, 0.02);
  border-radius: 30rpx;
  z-index: 2;
  margin: 0 auto;
  float: left;
  margin-left: 40rpx;
  margin-top: -20rpx;
}
.check-link-equip{
  height: 66rpx;
  width: 670rpx;
  margin-top: 30rpx;
  float: left;
}
.check-link-equip view{
  width: 24rpx;
  height: 24rpx;
  margin-top: 21rpx;
  margin-left: 32rpx;
  float: left;
  border-radius: 50%;
}
.check-link-equip image{
  height: 66rpx;
  width: 66rpx;
  float: left;
  margin-left: 4rpx;
}

.check-link-text{
  background-color: rgba(242, 242, 246, 0.5);
  border-radius: 8rpx;
  text-align: center;
}
